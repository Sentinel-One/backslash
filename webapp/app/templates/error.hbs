{{#tool-bar}}

  <p class="navbar-text">
    {{#link-to 'session' test.session.display_id}} Session {{test.session.display_id}}{{/link-to}} /
    {{#link-to 'test' test.display_id }}Test {{test.display_id}}{{/link-to}} /
    {{#link-to 'test.errors' test.display_id}} Errors {{/link-to}} / {{error.id}}
  </p>

{{/tool-bar}}


<div class="container">
  <table class="table">
    <tbody>
      <tr>
        <td><strong>Exception type</strong></td>
        <td>{{error.exception_type}}</td>
      </tr>
      <tr>
        <td><strong>Exception Message</strong></td>
        <td><code>{{error.message}}</code></td>
      </tr>
      <tr>
        <td><strong>Timestamp</strong></td>
        <td>{{moment-time unix=error.exception_type}}</td>
      </tr>
    </tbody>
  </table>
</div>

<div class="container">
  {{#each error.traceback as |frame|}}
    {{errors/traceback-frame frame=frame}}
  {{/each}}
</div>
