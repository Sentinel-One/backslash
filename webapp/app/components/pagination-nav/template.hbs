{{#if (not (eq page 1)) }}
  <a class="btn btn-default first" {{action "first_page"}} title="Go to first page"><i class="fa fa-angle-double-left"></i></a>
  <a class="btn btn-default first" {{action "prev_page"}} title="Go to previous page"><i class="fa fa-angle-left"></i></a>
{{/if}}

{{# if has_next}}
  <a class="btn btn-default next" {{action "next_page"}} title="Go to next page"><i class="fa fa-angle-right"></i></a>
{{/if}}
