{{#with session as |obj|}}
  {{partial "errors-failures-notice"}}
{{/with}}

<div class="row">

  <div class="col-xs-2">
    <img class="img-circle" title="Started by {{session.user_email}}" src="{{gravatar-url session.user_email}}"/>

  </div>

  <div class="col-xs-10">
    <div class="row">
      <div class="col-xs-6">
        <p><i class="fa fa-clock-o"></i> <strong>Started</strong>: {{moment-time unix=session.start_time}}</p>
        <p><i class="fa fa-clock-o"></i> <strong>Ended</strong>: {{moment-time unix=session.end_time}}</p>
      </div>
      <div class="col-md-6">
        {{#if metadata.slash }}
          <p><i class="fa fa-chevron-right"></i> <strong>Command line:</strong> <code>{{metadata.slash.commandline}}</code></p>
        {{/if}}
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12">
        {{#each session.subjects as |subject|}}
          <p>
            <i class="fa fa-barcode"></i> <strong>{{subject.name}}</strong> ({{subject.product}} v{{subject.version}}, r{{subject.revision}})
          </p>
        {{/each}}
      </div>

    </div>
  </div>

</div>
