
{{#tool-bar}}
  {{filter-control filter_config=filter_config filter_action="filter"}}
  <form class="navbar-form navbar-right">
    <button type="button" class="btn {{if show_archived 'btn-success'}}" {{action "toggle_show_archived"}}>Show Archived</button>
  </form>
{{/tool-bar}}


<div class="container">

  {{#each model as |session|}}
    {{ session-row session=session action="goto_session"}}
  {{/each}}
</div>

{{pagination-nav page=page pages_total=pages_total next_page="next_page" prev_page="prev_page" goto_page="goto_page"}}
